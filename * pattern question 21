
#include <iostream>
using namespace std;
int
main ()
{
  int n,a=1;
  cin >> n;

  int row = 1;
  while (row <= n)
    {
      int space = n - row;
      while (space)
	{
	  cout << " # ";
	  space--;
	}
      int col = 1;

      while (col <=row)
	{

	  cout <<a<<" " ;
	  a++;
	  col++;


	}

      cout << endl;
      row++;
    }



}
