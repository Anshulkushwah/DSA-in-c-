

/* method 1 */


#include <iostream>

using namespace std;



int
main ()
{ int size=5;
    int arr[size]={-2,1,-3,23,43};
 

for(int i=0;i<size;i++){
    if(arr[i]<0){
        cout<<arr[i]<<" ";
    }
}

for(int i=0;i<size;i++){
    if(arr[i]>0){
        cout<<arr[i]<<" ";
    }
}
    
    
  return 0;
}


/* method 2 */
#include <iostream>

using namespace std;



int
main ()
{
  int size = 5, j = 0;
  int arr[size] = { -2, 1, -3, 23, 43 };
  for (int i = 0; i < size; i++)
    {
      if (arr[i] < 0)
	{
	  swap (arr[i], arr[j]);
	  j++;
	}

    }
  for (int i = 0; i < size; i++)
    {

      cout << arr[i] << " ";
    }



  return 0;
}
