
#include <iostream>

using namespace std;

int fib(int n ){
    int ans;
    // Base case 
    
    if(n == 0) return 0;
    if(n == 1) return 1;
    
    //Processing
    
    // recursive call
    
    ans = fib(n-1)+fib(n-2);
    
    return ans;
    
    
    
}

int
main ()
{
  cout << "Enter the number ";
  int n;
  cin >> n;
 cout<< fib (n);

  return 0;
}
