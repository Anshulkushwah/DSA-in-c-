
#include <iostream>

using namespace std;

int main()
{   // creation 
    string str ;
    
    // intput 
    cin >> str ; 
    // take only first word of string after space,newline don't take
    // getline(cin, str) -- > is used for take input full sentence // we can also take custom delimeter in this function
    
    // print 
    cout<<"str is " << str << endl; 
    
    return 0;
}
