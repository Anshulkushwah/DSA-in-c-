
#include <iostream>

using namespace std;



int
main ()
{
  int size = 5, ans1 = 0, ans2,ans;
  int arr[size] = { 1, 2, 3, 3, 4 };
  for (int i = 0; i < size; i++)
    {

      ans1 = ans1 + arr[i];
    }
    
    ans2= 4*(4+1)/2;
    ans=ans1-ans2;
  cout << "Duplicate value is " <<ans;





  return 0;
}
