
#include <iostream>

using namespace std;


#include <iostream>

using namespace std;


int
gcd (int a, int b)
{ // agr a zero ke equal hai to a return krdo
  if (a == 0)
    return b;
// agr b zero ke equal hai to b return krdo
  if (b == 0)
    return a;

  while (a != b)
    {
      if (a > b)
	a = a - b;

      else
	b = b - a;

    }
  return a;

}



int
main ()
{
  int a, b;
  cout << "Enter the number a & b";
  cin >> a >> b;

  cout << "GCD is -- > " gcd (a, b);
}
