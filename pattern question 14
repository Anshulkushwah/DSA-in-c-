
#include <iostream>
using namespace std;
int
main ()
{
  int n;
  cout<<"enter no.";
  cin>>n;
  
  
  int i= 1;
  while(i<=n){
      int j=1;
      char ch=65+n-i;
      while(j<=i){
          
          cout<<ch;
          ch++;
          j++;
          
      }
      cout<<endl;
     
      i++;
  }
  
} 
