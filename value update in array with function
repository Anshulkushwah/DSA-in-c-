
#include <iostream>
#include <climits>
using namespace std;

void update(int arr[],int n)
{
    cout<<"inside funtion"<<endl;
    arr[0]=543;
    for(int i = 0; i<n;i++)
    {
        cout<<arr[i]<<" ";
    }cout<<endl;
    cout<<"going back"<<" ";
    
}





int
main ()
{
  int arr[3]={1,2,3}; 
  

  update(arr,3); 
for(int i = 0; i<3;i++)
    {
        cout<<arr[i]<<" ";
    }cout<<endl;
}
